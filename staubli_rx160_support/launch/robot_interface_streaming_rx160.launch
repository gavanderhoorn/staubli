<!--
	Manipulator specific version of 'robot_interface_streaming.launch'.

	Defaults provided for RX160:
	 - 6 joints

	Usage:
		robot_interface_streaming_rx160.launch robot_ip:=<value>
-->
<launch>
	<arg name="robot_ip" />

	<rosparam command="load" file="$(find staubli_rx160_support)/config/joint_names_rx160.yaml" />

	<include file="$(find staubli_val3_driver)/launch/robot_interface_streaming.launch">
		<arg name="robot_ip" value="$(arg robot_ip)" />
	</include>
</launch>
